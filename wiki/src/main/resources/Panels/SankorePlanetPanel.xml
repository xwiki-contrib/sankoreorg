<content>{{include document="CurrikiCode.BrowseMacros" /}}
{{velocity filter="none"}}
{{html clean="false" wiki="true"}}
&lt;div id="planete" class="block block-block clearfix"&gt;
  &lt;img width="81px" src="/xwiki/bin/download/Panels/SankorePlanetPanel/BGplanete.png" typeof="foaf:Image" class="block-icon"&gt;
  &lt;h2 class="block-title"&gt;Planète Sankoré&lt;/h2&gt;
  &lt;div class="block-contents-top"&gt;
    {{html}}
    #template("logininline.vm")     
    {{/html}}
    &lt;div class="clearfloats"&gt;&lt;/div&gt;
  #if(!$isGuest)
    &lt;p&gt;&lt;a class="download-link" href="javascript:Curriki.module.addpath.startPath('B');"&gt;Ajoutez une resource&lt;/a&gt;&lt;/p&gt;
  #end
  &lt;/div&gt;
  &lt;div class="block-contents-bottom"&gt;
    &lt;h3&gt;Naviguer par sujets&lt;/h3&gt;
    &lt;div class="browse-page"&gt;
      &lt;p class="browse-text"&gt;$msg.get("browse.info")&lt;/p&gt;
      #foreach($subj in $subjects)
        #simplesubjectcard($subj)
      #end
    &lt;/div&gt;
  &lt;/div&gt;
&lt;/div&gt;
{{/html}}
{{/velocity}}</content>
